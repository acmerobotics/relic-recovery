<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <!-- REV -->
    <LynxUsbDevice name="portal" serialNumber="DQ1NXDE5" parentModuleAddress="2">
        <LynxModule name="rearHub" port="2">
            <NeveRest20Gearmotor name="frontLeft" port="0" />
            <NeveRest20Gearmotor name="rearLeft" port="1" />
            <NeveRest20Gearmotor name="rearRight" port="2" />
            <NeveRest20Gearmotor name="frontRight" port="3" />
            <Servo name="intakeFlipper" port="0" />
            <Servo name="intakeGripperRight" port="2" />
            <Servo name="relicFinger" port="4" />
            <Servo name="relicWrist" port="5" />
            <DigitalDevice name="dumpLiftTopTouch" pduort="1" />
            <DigitalDevice name="dumpLiftBottomTouch" port="3" />
            <DigitalDevice name="intakeTouch" port="5" />
            <LynxEmbeddedIMU name="imu" port="0" bus="0" />
        </LynxModule>
    </LynxUsbDevice>

    <!-- MR -->
    <MotorController name="intakeRelicMotorController" serialNumber="AI049PHE">
        <RevRoboticsCoreHexMotor name="intakeRotate" port="1" />
        <NeveRest20Gearmotor name="relicExtend" port="2" />
    </MotorController>
    <MotorController name="dumpMotorController" serialNumber="AH00QHU3">
        <RevRoboticsCoreHexMotor name="dumpLiftRight" port="1" />
        <RevRoboticsCoreHexMotor name="dumpLiftLeft" port="2" />
    </MotorController>
    <ServoController name="servoController" serialNumber="AI02RKXM">
        <Servo name="intakeGripperLeft" port="1" />
        <Servo name="dumpRotateLeft" port="2" />
        <Servo name="dumpRotateRight" port="3" />
        <Servo name="dumpRelease" port="4" />
        <Servo name="jewelDeployer" port="5" />
        <Servo name="jewelSlapper" port="6" />
    </ServoController>
</Robot>

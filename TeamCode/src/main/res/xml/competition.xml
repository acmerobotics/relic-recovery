<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice name="portal" serialNumber="DQ1NXDE5" parentModuleAddress="1">
        <LynxModule name="rearHub" port="1">
            <NeveRest20Gearmotor name="frontLeft" port="0" />
            <NeveRest20Gearmotor name="rearLeft" port="1" />
            <NeveRest20Gearmotor name="rearRight" port="2" />
            <NeveRest20Gearmotor name="frontRight" port="3" />
            <Servo name="dumpRotate1" port="0" />
            <Servo name="dumpRotate2" port="1" />
            <Servo name="dumpRelease" port="2" />
            <Servo name="dumpFlipper" port="3" />
            <LynxEmbeddedIMU name="imu" port="0" bus="0" />
        </LynxModule>
        <LynxModule name="frontHub" port="2">
            <!-- Intake motors go in ports 0 and 1 -->
            <RevRoboticsCoreHexMotor name="dumpLift" port="2" />
            <NeveRest20Gearmotor name="relicExtend" port="3" />
            <Servo name="relicFlipper" />
            <Servo name="relicGrabber" />
            <Servo name="jewelDeployer" />
            <Servo name="jewelSlapper" />
        </LynxModule>
    </LynxUsbDevice>
</Robot>
